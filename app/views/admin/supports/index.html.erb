<% breadcrumb :admin_support %>

<!-- スマートフォンタイトル　-->
<h5 class="visible-xs visible-sm">
  <span class="glyphicon glyphicon-th-large" aria-hidden="true"></span>
  <%= t("system_usage.SYS_CONFIRMSYSTEMLOG_NAVI") %>
</h5>

<ul class="nav nav-tabs support-tab">
	<li class="active"><a href="#support-monthly" data-toggle="tab"><%= t("support.SUPPORT_MONTHLY_GRAPH") %></a></li>
	<li><a href="#support-score" data-toggle="tab"><%= t("support.SUPPORT_SCORE_GRAPH") %></a></li>
  <li><a href="#support-detail" data-toggle="tab"><%= t("support.SUPPORT_DETAIL") %></a></li>
</ul>

<div class="tab-content">
	<div class="tab-pane active" id="support-monthly">
    <%= render 'monthly' %>
	</div>

	<div class="tab-pane" id="support-score">
    <%= render 'score' %>
	</div>

  <div class="tab-pane" id="support-detail">
		<%= render 'detail' %>
	</div>
</div>
