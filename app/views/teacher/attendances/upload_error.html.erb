<% breadcrumb :admin_user_upload_results %>

<div class="container-fluid">
  <!-- 説明文 -->
  <div class="row">
    <div class="col-sm-12">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title">
            <%= t("registerList.PRI_REG_ERRORLIST_TEXT") %>
          </h3>
        </div>
        <div class="panel-body">
          <table>
            <tbody>
              <% @errors.each do |key, value| %>
              <tr>
                <td><%= key %>:<%= value %></td>
              </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>

      <div class="text-center">
        <div class="btn-group">
          <%= link_to t("common.COMMON_CLOSE"), "javascript:void();", {:class => "btn btn-default ceas-button-margin-5", onclick: "window.close()"} %>
        </div>
      </div>
    </div>
  </div>
</div>
