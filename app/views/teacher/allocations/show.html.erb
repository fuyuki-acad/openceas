            </div>
            <div id="collapseListEvaluationlist" class="panel-collapse collapse" role="tabpanel" aria-labelledby="collapseListEvaluationlistHeading">
              <div class="panel-body">

                <%= render 'list', :generic_pages => @course.evaluations %>

                <p>
                  <% if @course.questionnaires.count == 0 %>
                    <%= t("select_class_session.MAT_ALL_SELECTCLASSSESSION_MATERIALTEXT21") %>
                  <% end %>
                </p>

                <% if @course.questionnaires.count > 0 %>
